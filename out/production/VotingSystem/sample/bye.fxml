<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.byeController">
    <children>
        <AnchorPane prefHeight="159.0" prefWidth="600.0">
            <children>
                <ImageView fx:id="endImageView" fitHeight="150.0" fitWidth="206.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../Images/vote45.jpg" />
                    </image>
                </ImageView>
                <Label alignment="CENTER" layoutX="158.0" layoutY="33.0" prefHeight="92.0" prefWidth="442.0" text="Electoral Commission Ghana" textFill="#f8f4f4">
                    <font>
                        <Font name="System Bold" size="24.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
      <Button fx:id="closevButton" layoutX="400.0" layoutY="299.0" mnemonicParsing="false" onAction="#CloseButtonOnAction" prefHeight="19.0" prefWidth="135.0" text="Close">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="homeButton" layoutX="64.0" layoutY="299.0" mnemonicParsing="false" onAction="#homebuttonOnAction" prefHeight="19.0" prefWidth="135.0" text="Home page">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="23.0" layoutY="179.0" prefHeight="92.0" prefWidth="550.0" text="Thank you for your vote!" textFill="#f8f3f3">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
